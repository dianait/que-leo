{
  "name": "article-reader",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest --config jest.config.cjs",
    "test:e2e": "dotenv -e .env.test -- playwright test",
    "test:setup": "chmod +x scripts/setup-test-env.sh && ./scripts/setup-test-env.sh",
    "convert-to-webp": "tsx scripts/convert-to-webp.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "npm test -- --findRelatedTests --passWithNoTests"
    ]
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "date-fns": "4.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "7.6.3",
    "sharp": "0.34.2"
  },
  "devDependencies": {
    "@eslint/js": "9.25.0",
    "@playwright/mcp": "0.0.29",
    "@playwright/test": "1.53.2",
    "@supabase/supabase-js": "2.50.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react-swc": "3.9.0",
    "dotenv-cli": "8.0.0",
    "eslint": "9.25.0",
    "eslint-plugin-hexagonal-architecture": "1.0.3",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "jest": "30.0.2",
    "jest-environment-jsdom": "30.0.2",
    "lint-staged": "16.1.2",
    "ts-jest": "29.4.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.30.1",
    "vite": "^6.4.1"
  }
}
